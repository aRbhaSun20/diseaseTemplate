  "Diagnosis": {
    "description 0": {
      "text": "Generally abdominal pain goes with time, but if the pain does not subside, then one should seek medical help:",
      "fontWeight": "normal",
      "fontSize": 16,
      "type": "description 0"
    },
    "list 1": {
      "text": "Abdominal discomfort that lasts 1 week or longer\t",
      "fontWeight": "normal",
      "fontSize": 16,
      "type": "list 1"
    },
    "list 2": {
      "text": "Abdominal pain that does not improve in 24 - 48 hours, or becomes more severe and frequent and occurs with nausea and vomiting\t",
      "fontWeight": "normal",
      "fontSize": 16,
      "type": "list 2"
    },
    "list 3": {
      "text": "Bloating that persists for more than 2 days\t",
      "fontWeight": "normal",
      "fontSize": 16,
      "type": "list 3"
    },
    "list 4": {
      "text": "Burning sensation or increase in frequency on urination\t",
      "fontWeight": "normal",
      "fontSize": 16,
      "type": "list 4"
    },
    "list 5": {
      "text": "Diarrhoea for more than 5 days\t",
      "fontWeight": "normal",
      "fontSize": 16,
      "type": "list 5"
    },
    "list 6": {
      "text": "Fever (over 100°F for adults or 100.4°F for children) with pain\t",
      "fontWeight": "normal",
      "fontSize": 16,
      "type": "list 6"
    },
    "list 7": {
      "text": "Prolonged poor appetite\t",
      "fontWeight": "normal",
      "fontSize": 16,
      "type": "list 7"
    },
    "list 8": {
      "text": "Prolonged vaginal bleeding\t",
      "fontWeight": "normal",
      "fontSize": 16,
      "type": "list 8"
    },
    "list 9": {
      "text": "Unexplained weight loss\t",
      "fontWeight": "normal",
      "fontSize": 16,
      "type": "list 9"
    }
  },
